# Vidalytics Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
NUXT_PUBLIC_API_BASE_URL=http://localhost:8000
NUXT_PUBLIC_WS_URL=ws://localhost:8000

# Environment
NUXT_PUBLIC_ENVIRONMENT=development

# Feature Flags
NUXT_PUBLIC_ENABLE_ANALYTICS=true
NUXT_PUBLIC_ENABLE_PERFORMANCE_MONITOR=false
NUXT_PUBLIC_ENABLE_DEBUG_MODE=true

# External Services
NUXT_PUBLIC_YOUTUBE_API_KEY=your_youtube_api_key_here
NUXT_PUBLIC_GOOGLE_CLIENT_ID=your_google_client_id_here

# Supabase (if using)
NUXT_PUBLIC_SUPABASE_URL=your_supabase_url_here
NUXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Analytics (optional)
NUXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
NUXT_PUBLIC_HOTJAR_ID=your_hotjar_id_here

# Error Reporting (optional)
NUXT_PUBLIC_SENTRY_DSN=your_sentry_dsn_here

# Development
NUXT_DEVTOOLS=true
NUXT_TELEMETRY_DISABLED=true
