# Multi-Agent System API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
GOOGLE_API_KEY=your_google_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# YouTube Data API v3
YOUTUBE_API_KEY=your_youtube_api_key_here

# Boss Agent Authentication (generate a secure random key)
BOSS_AGENT_SECRET_KEY=your_secure_random_key_here

# Google OAuth 2.0 Configuration (get from Google Cloud Console)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here
OAUTH_REDIRECT_URI=http://localhost:8888/auth/callback

# Agent System Configuration
AGENT_CACHE_SIZE=5000
AGENT_CACHE_TTL=7200
MAX_CONCURRENT_AGENTS=10
API_RATE_LIMIT=100

# Environment (development, staging, production)
ENVIRONMENT=development