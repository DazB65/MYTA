# MYTA Environment Configuration Template
# Copy this file to .env and fill in your actual API keys and configuration
# NEVER commit the .env file to version control

# =============================================================================
# AI/ML API Keys (Required for agent functionality)
# =============================================================================

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=sk-proj-your_openai_api_key_here

# Anthropic API Key - Get from https://console.anthropic.com/
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_api_key_here

# Google AI API Key - Get from https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_api_key_here

# Gemini API Key - Get from https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# =============================================================================
# OAuth Configuration (Required for Google/YouTube integration)
# =============================================================================

# Google OAuth Client ID - Get from https://console.cloud.google.com/
GOOGLE_CLIENT_ID=your_google_client_id.apps.googleusercontent.com

# Google OAuth Client Secret
GOOGLE_CLIENT_SECRET=your_google_client_secret

# OAuth Redirect URI (adjust for your environment)
OAUTH_REDIRECT_URI=http://localhost:8000/auth/callback

# =============================================================================
# YouTube Data API v3 (Required for YouTube analytics)
# =============================================================================

# YouTube API Key - Get from https://console.cloud.google.com/
YOUTUBE_API_KEY=your_youtube_api_key_here
YT_API_KEY=your_youtube_api_key_here

# =============================================================================
# Security Keys (Generate secure random keys)
# =============================================================================

# Boss Agent Secret Key (64 character hex string)
# Generate with: openssl rand -hex 32
BOSS_AGENT_SECRET_KEY=your_64_character_hex_secret_key_here

# Session Secret Key (64 character hex string)
# Generate with: openssl rand -hex 32
SESSION_SECRET_KEY=your_64_character_hex_session_key_here

# =============================================================================
# Supabase Configuration (Required for database)
# =============================================================================

# Supabase Project URL - Get from your Supabase dashboard
VITE_SUPABASE_URL=https://your_project_id.supabase.co

# Supabase Anonymous Key - Get from your Supabase dashboard
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# =============================================================================
# Agent System Configuration
# =============================================================================

# Agent cache configuration
AGENT_CACHE_SIZE=5000
AGENT_CACHE_TTL=7200
MAX_CONCURRENT_AGENTS=10
API_RATE_LIMIT=100

# =============================================================================
# Environment Configuration
# =============================================================================

# Environment: development, staging, production
ENVIRONMENT=development

# Debug mode (true/false)
DEBUG=false

# Log level: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# =============================================================================
# Database Configuration
# =============================================================================

# Database URL (SQLite for development, PostgreSQL for production)
DATABASE_URL=sqlite:///./Vidalytics.db

# Database echo (log SQL queries)
DATABASE_ECHO=false

# =============================================================================
# Redis Configuration (Optional - for session management and caching)
# =============================================================================

# Redis connection URL
REDIS_URL=redis://localhost:6379

# Redis password (if required)
REDIS_PASSWORD=

# Redis database number
REDIS_DB=0

# Redis connection pool size
REDIS_MAX_CONNECTIONS=10

# =============================================================================
# CORS Configuration
# =============================================================================

# Allowed origins for CORS (JSON array format)
CORS_ORIGINS=["http://localhost:3000"]

# =============================================================================
# Security Configuration
# =============================================================================

# Strict security headers (true/false)
SECURITY_HEADERS_STRICT=true

# Strict CSRF protection (true/false)
CSRF_PROTECTION_STRICT=true

# =============================================================================
# Rate Limiting Configuration
# =============================================================================

# Rate limits for API endpoints
RATE_LIMIT_PER_MINUTE=60
RATE_LIMIT_PER_HOUR=1000

# =============================================================================
# Backup Configuration (Optional)
# =============================================================================

# Backup frequency: daily, weekly, monthly
BACKUP_FREQUENCY=daily

# Backup time (24-hour format)
BACKUP_TIME=02:00

# Enable backup compression
BACKUP_COMPRESSION=true

# Maximum number of backups to keep
BACKUP_MAX_COUNT=7

# Enable automatic cleanup of old backups
BACKUP_CLEANUP_ENABLED=true

# Backup directory path
BACKUP_DIRECTORY=./backups

# =============================================================================
# Production-Only Configuration (Uncomment for production)
# =============================================================================

# # Production database URL (PostgreSQL recommended)
# DATABASE_URL=postgresql://username:password@localhost:5432/myta_production

# # Production Redis URL (with authentication)
# REDIS_URL=redis://username:password@redis-server:6379

# # Production CORS origins
# CORS_ORIGINS=["https://yourdomain.com", "https://www.yourdomain.com"]

# # Production OAuth redirect URI
# OAUTH_REDIRECT_URI=https://yourdomain.com/auth/callback

# # SSL/TLS Configuration
# SSL_CERT_PATH=/path/to/ssl/cert.pem
# SSL_KEY_PATH=/path/to/ssl/key.pem

# # External monitoring/logging services
# SENTRY_DSN=your_sentry_dsn_here
# DATADOG_API_KEY=your_datadog_api_key_here

# =============================================================================
# Development-Only Configuration
# =============================================================================

# Enable development features
DEV_MODE=true

# Hot reload for development
HOT_RELOAD=true

# Development server port
DEV_PORT=8000

# Frontend development server URL
FRONTEND_URL=http://localhost:3000

# =============================================================================
# Security Alerting Configuration
# =============================================================================

# Enable/disable security alerts
SECURITY_ALERTS_EMAIL_ENABLED=false
SECURITY_ALERTS_SLACK_ENABLED=false
SECURITY_ALERTS_WEBHOOK_ENABLED=false

# Email configuration for security alerts
SMTP_SERVER=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password
ALERT_EMAIL_FROM=security@myta.com
ALERT_EMAIL_TO=admin@myta.com,security@myta.com

# Slack webhook for security alerts
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Generic webhook for security alerts
SECURITY_WEBHOOK_URL=https://your-monitoring-system.com/webhook
